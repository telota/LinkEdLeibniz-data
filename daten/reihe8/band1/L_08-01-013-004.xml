						<rdg wit="#L" n="1"> Esto ergo problema. </rdg>
						<rdg wit="#L" n="2"> (4) <hi rendition="#g">Magnetis</hi><index indexName="Sachverzeichnis">
							<term>magnes</term>
							</index><hi rendition="#g"> </hi><hi rendition="#g">declinationes</hi><index indexName="Sachverzeichnis">
							<term>declinatio</term>
