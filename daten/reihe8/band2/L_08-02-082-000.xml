						</row>
						<row>
							<cell>50<!--  \hackfor{\edtext{}{\lemma{50}\Bfootnote{\textit{(1)} 20 \textit{(2)} 30 \textit{L}}}} --><app type="Bfootnote"><lem type="main"/><note type="lemma-abbrev">50</note><rdg wit="#L" n="1"> 20 </rdg><rdg wit="#L" n="2"> 30 <hi rendition="#i">L</hi></rdg></app> <lb/> 60<!--  \hackfor{\edtext{}{\lemma{60}\Bfootnote{\textit{(1)} 24,30 \textit{(2)} 34,30 \textit{L}}}} --><app type="Bfootnote"><lem type="main"/><note type="lemma-abbrev">60</note><rdg wit="#L" n="1"> 24,30 </rdg><rdg wit="#L" n="2"> 34,30 <hi rendition="#i">L</hi></rdg></app></cell>
							<cell>30<lb/>34,30</cell>
