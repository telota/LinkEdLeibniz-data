					<anchor xml:id="_38_135_1a"/>
					<p xml:id="Ch79.p18"><hi rendition="#i">Appendatur</hi><app type="Cfootnote" from="#_38_135_1a" to="#_38_135_1b">
						<lem type="main"/>
						<note type="lemma-abbrev"><hi rendition="#i">Appendatur</hi> <seg type="apparat-ellipsis"/> <hi rendition="#i">nostratium</hi></note>
						<note type="editorial"><bibl corresp="#bib_00546">a.a.O., S. 607.</bibl></note>
						</app> <hi rendition="#i">globulus ferreus duarum librarum funiculo cujusvis longitudinis, quem modo breviorem modo longiorem facies</hi>[,] <hi rendition="#i">donec 16 exacte eveniant vibrationes, spatio</hi> <app type="Bfootnote">
						<!--is-correction-app-with-lemma-->
						<lem type="main"><corr resp="#ed"><hi rendition="#i">vig</hi>[<hi rendition="#i">i</hi>]<hi rendition="#i">nti</hi></corr></lem>
