               <optional>
                  <ref name="tei_bloc"/>
               </optional>
               <optional>
                  <ref name="tei_country"/>
               </optional>
               <optional>
                  <ref name="tei_region"/>
               </optional>
               <optional>
                  <ref name="tei_settlement"/>
               </optional>
               <optional>
                  <ref name="tei_district"/>
               </optional>
               <optional>
                  <ref name="tei_geogName"/>
               </optional>
               <optional>
                  <ref name="tei_institution"/>
               </optional>
               <optional>
                  <ref name="tei_repository"/>
